{
  "name": "create-airbnb-x-config",
  "version": "3.0.0",
  "description": "Airbnb Extended Config CLI",
  "keywords": [
    "eslint",
    "airbnb",
    "eslint config cli",
    "eslint config cli x",
    "eslint config cli extended",
    "airbnb cli",
    "airbnb config",
    "airbnb config cli",
    "airbnb config cli x",
    "airbnb config cli extended"
  ],
  "homepage": "https://eslint-airbnb-extended.nishargshah.dev",
  "bugs": {
    "url": "https://github.com/NishargShah/eslint-config-airbnb-extended/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/NishargShah/eslint-config-airbnb-extended.git",
    "directory": "packages/create-airbnb-x-config"
  },
  "license": "MIT",
  "author": "Nisharg Shah <nishargshah3101@gmail.com>",
  "type": "module",
  "exports": {
    ".": "./dist/index.mjs",
    "./package.json": "./package.json"
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "bin": {
    "create-airbnb-extended-config": "./dist/index.mjs",
    "create-airbnb-x-config": "./dist/index.mjs"
  },
  "scripts": {
    "build": "tsdown",
    "build:dev": "tsdown --watch",
    "dev": "tsx index.ts",
    "format": "prettier . --check --log-level silent --ignore-path ../../.gitignore",
    "format:fix": "prettier . --write --log-level silent --ignore-path ../../.gitignore",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "start": "node ./dist/index.mjs",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "commander": "^14.0.2",
    "cross-spawn": "^7.0.6",
    "package-manager-detector": "^1.6.0",
    "picocolors": "^1.1.1",
    "prompts": "^2.4.2"
  },
  "devDependencies": {
    "@types/cross-spawn": "^6.0.6",
    "@types/prompts": "^2.4.9",
    "tsdown": "catalog:",
    "tsx": "catalog:"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
