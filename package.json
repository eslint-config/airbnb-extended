{
  "name": "@airbnb-extended/project",
  "version": "3.0.0",
  "description": "Eslint Airbnb Config Extended Project",
  "homepage": "https://eslint-airbnb-extended.nishargshah.dev",
  "bugs": {
    "url": "https://github.com/eslint-config/airbnb-extended/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/eslint-config/airbnb-extended.git"
  },
  "license": "MIT",
  "author": "Nisharg Shah <nishargshah3101@gmail.com>",
  "type": "module",
  "scripts": {
    "build": "pnpm -r build",
    "clean:all": "pnpm clean:node_modules && pnpm clean:generated-folders",
    "clean:dist": "pnpx rimraf dist && pnpm -r exec pnpx rimraf dist",
    "clean:generated-folders": "pnpm clean:dist",
    "clean:node_modules": "pnpx rimraf node_modules && pnpm -r exec pnpx rimraf node_modules",
    "clean:workspace": "pnpm clean:node_modules && pnpx rimraf pnpm-lock.yaml",
    "cli:build": "pnpm --filter create-airbnb-x-config build",
    "cli:dev": "pnpm --filter create-airbnb-x-config dev",
    "cli:start": "pnpm --filter create-airbnb-x-config start",
    "config:build": "pnpm --filter eslint-config-airbnb-extended build",
    "docs:build": "pnpm --filter @airbnb-extended/docs build",
    "docs:dev": "pnpm --filter @airbnb-extended/docs dev",
    "docs:preview": "pnpm --filter @airbnb-extended/docs preview",
    "format": "pnpm root:format && pnpm -r format",
    "format:fix": "pnpm root:format:fix && pnpm -r format:fix",
    "fresh:init": "pnpm clean:workspace && pnpm clean:generated-folders",
    "lint": "pnpm root:lint && pnpm -r lint",
    "lint:fix": "pnpm root:lint:fix && pnpm -r lint:fix",
    "lint:inspector": "pnpx @eslint/config-inspector",
    "pkg:prepare": "pnpm changeset && pnpm changeset version",
    "pkg:publish": "pnpm -r build && pnpm changeset publish",
    "prepare": "husky || true",
    "root:format": "prettier . --check --log-level silent",
    "root:format:fix": "prettier . --write --log-level silent",
    "root:lint": "eslint .",
    "root:lint:fix": "eslint . --fix",
    "script:lint": "bash -e ./scripts/lint.sh",
    "templates:build": "pnpm --filter @airbnb-extended/build-templates build",
    "templates:dev": "pnpm --filter @airbnb-extended/build-templates dev",
    "typecheck": "pnpm -r typecheck"
  },
  "devDependencies": {
    "@airbnb-extended/eslint-config": "workspace:*",
    "@airbnb-extended/lint-staged-config": "workspace:*",
    "@airbnb-extended/prettier-config": "workspace:*",
    "@changesets/cli": "^2.29.8",
    "@types/node": "^24.10.9",
    "eslint": "catalog:",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.0",
    "prettier-plugin-packagejson": "^3.0.0",
    "tsc-files": "^1.1.4",
    "typescript": "catalog:"
  },
  "packageManager": "pnpm@10.28.1",
  "engines": {
    "node": "^18.18.0 || ^20.9.0 || >=21.1.0",
    "npm": "please-use-pnpm",
    "pnpm": ">=10.26.0",
    "yarn": "please-use-pnpm"
  }
}
